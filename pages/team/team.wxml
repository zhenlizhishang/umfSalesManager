<view class="page">
    <view class="page__bd">
        <view class="weui-cells weui-cells_after-title">
          <view wx:for='{{teams}}' wx:for-item='team'>
            <navigator url="team.reqUrl" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="{{team.imgUrl}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">{{team.username}}</view>
                <view class="weui-cell__bd team_role">{{team.role}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access">{{team.phone}}</view>
            </navigator>
          </view>
        </view>
    </view>
</view>